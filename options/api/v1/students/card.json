{
  "id": "/api/v1/student/card",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "cards",
  "type": "object",
  "name": "cards",
  "description": "A card is a simple way to display content types across the site, they can be read, written. Cards break up into mobile format easily and can pile vertically or horizontally.",
  "properties": {
    "data": {
      "type": "object",
      "oneOf": [
        {
          "$ref": "/api/v1/card/map/small.json"
        },
        {
          "$ref": "/api/v1/card/media/medium.json"
        },
        {
          "$ref": "/api/v1/card/media/small.json"
        },
        {
          "$ref": "/api/v1/card/media/media.json"
        },
        {
          "$ref": "/api/v1/card/form/small.json"
        },
        {
          "$ref": "/api/v1/card/form/medium.json"
        },
        {
          "$ref": "/api/v1/card/form/large.json"
        },
        {
          "$ref": "/api/v1/card/range/small.json"
        },
        {
          "$ref": "/api/v1/card/dial/small.json"
        },
        {
          "$ref": "/api/v1/card/image/small.json"
        },
        {
          "$ref": "/api/v1/card/image/medium.json"
        },
        {
          "$ref": "/api/v1/card/video/medium.json"
        },
        {
          "$ref": "/api/v1/card/qrcode/medium.json"
        },
        {
          "$ref": "/api/v1/card/weather/medium.json"
        }
      ]
    },
    "mediaType": {
      "title": "Media Type",
      "description": "Type of the data",
      "identity": false,
      "type": "enum",
      "enum": [
        "map/small",
        "media/medium",
        "media/small",
        "media/media",
        "form/small",
        "form/medium",
        "form/large",
        "range/small",
        "dial/small",
        "image/small",
        "image/medium",
        "video/medium",
        "qrcode/medium",
        "weather/medium"
      ]
    },
    "updated": {
      "title": "updated",
      "description": "updated",
      "identity": false,
      "readonly": false,
      "type": "date-time"
    },
    "_id": {
      "title": "_id",
      "description": "_id",
      "identity": true,
      "readonly": true,
      "type": "string"
    }
  },
  "links": {
    "self": {
      "rel": "self",
      "href": "/api/v1/cards/{{_id}}"
    },
    "destroy": {
      "rel": "destroy",
      "href": "/api/v1/cards/{{_id}}",
      "method": "DELETE"
    },
    "update": {
      "title": "Update a Card",
      "rel": "update",
      "href": "/api/v1/cards/{{_id}}",
      "properties": {
        "data": {
          "title": "data",
          "description": "data ",
          "identity": false,
          "readonly": false,
          "type": "any"
        },
        "updated": {
          "title": "updated",
          "description": "updated ",
          "identity": false,
          "readonly": false,
          "type": "date-time"
        }
      },
      "method": "PUT"
    },
    "root": {
      "rel": "root",
      "href": "#/result"
    }
  }
}