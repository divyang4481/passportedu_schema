{
  "id": "/api/v1/admissions",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Welcome, Admissions Teams",
  "type": "object",
  "description": "Please register to receive an account with us and manage your school.",
  "properties": {
  },
  "links": [
    {
      "title": "Home",
      "description": "The root resource for Passport EDU",
      "rel": "home",
      "importance": "navigation",
      "method": "GET",
      "href": "/api/v1"
    },
    {
      "title": "Admissions",
      "rel": "self",
      "importance": "navigation",
      "method": "GET",
      "href": "/api/v1/admissions"
    },
    {
      "title": "Sign In",
      "rel": "login",
      "importance": "navigation",
      "method": "GET",
      "href": "/api/v1/admissions/login"
    },
    {
      "title": "Register",
      "rel": "register",
      "importance": "content",
      "template": "/templates/api/v1/card/admissions/register",
      "href": "/api/v1/admissions/register",
      "method": "POST",
      "properties": {
        "fullName": {
          "title": "Your Full Name",
          "description": "First name and last name",
          "type": "string"
        },
        "username": {
          "title": "Your Email Address",
          "description": "A valid email address",
          "type": "email"
        },
        "password": {
          "title": "Your password",
          "type": "password"
        }
      },
      "required": ["username", "password", "email"]
    }
  ]
}