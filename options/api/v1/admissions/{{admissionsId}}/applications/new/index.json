{
  "id": "/api/v1/admissions/{{admissionsId}}/applications",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "A New Application",
  "type": "object",
  "description": "Students can apply to a program with this application.",
  "properties": {
    "admissionsId": {
      "title": "Unique ID of an Admissions Agent",
      "readonly": true,
      "identity": true,
      "type": "string"
    }
  },
  "links": [
    {
      "title": "My Dashboard",
      "rel": "myHome",
      "importance": "navigation",
      "method": "GET",
      "href": "/api/v1/admissions/{{admissionsId}}"
    },
    {
      "title": "Application Types",
      "rel": "types",
      "importance": "navigation",
      "method": "GET",
      "href": "/api/v1/admissions/{{admissionsId}}/applications"
    },
    {
      "title": "Application Types",
      "rel": "self",
      "importance": "navigation",
      "method": "GET",
      "href": "/api/v1/admissions/{{admissionsId}}/applications/new"
    },
    {
      "title": "Create a New Application Type",
      "rel": "create",
      "importance": "content",
      "template": "/templates/api/v1/card/admissions/newApplication",
      "method": "POST",
      "href": "/api/v1/admissions/{{admissionsId}}/applications",
      "properties": {
        "type": {
          "title": "Program Name",
          "type": "string"
        }
      }
    }
  ]
}