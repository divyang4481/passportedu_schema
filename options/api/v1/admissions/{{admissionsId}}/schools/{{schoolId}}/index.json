          "identity": false,
          "readonly": false,
          "type": "number"
        },
        "data.normalFeesTuition": {
          "title": "Normal Fees & Tuition",
          "description": "The cost of attending for an in-State admissions",
          "identity": false,
          "readonly": false,
          "type": "number"
        },
        "data.outOfStateTuition": {
          "title": "Out Of State Tuition",
          "description": "The cost of tuition for an international admissions",
          "identity": false,
          "readonly": false,
          "type": "number"
        },
        "data.outOfStateFees": {
          "title": "Out Of State Fees ",
          "description": "The fees for an international admissions",
          "identity": false,
          "readonly": false,
          "type": "number"
        },
        "data.SATCriticalReading25": {
          "title": "SAT Critical Reading 25th Percentile",
          "description": "The lower end of the scores accepted",
          "identity": false,
          "readonly": false,
          "type": "number"
        },
        "data.SATCriticalReading75": {
          "title": "SAT Critical Reading 75the Percentile",
