          "identity": false,
          "readonly": false,
          "type": "number"
        },
        "data.percentFemaleStudent": {
          "title": "Percentage Female Students",
          "description": "Percentage of admissions body identifying as female",
          "identity": false,
          "readonly": false,
          "type": "number"
        },
        "data.percentAdmitted": {
          "title": "Percentage Admitted",
