  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "My Dashboard",
  "type": "object",
  "description": "Your School {{school.data.institutionName}}",
  "properties": {
    "admissionsId": {
      "title": "Unique ID of an Admissions Agent",
      "readonly": true,
      "identity": true,
      "type": "string"
    },
    "schoolId": {
      "title": "Unique ID of a School",
      "readonly": true,
      "identity": true,
      "type": "string"
    },
    "school": {
      "type": "object",
      "properties": {
        "_id": {
          "title": "_id ",
          "description": "_id ",
          "identity": true,
          "readonly": true,
          "type": "string"
        },
        "data.unitId": {
          "title": "Unit Id",
          "description": "Government Identifier",
          "identity": false,
          "readonly": false,
          "type": "number"
        },
        "data.institutionName": {
          "title": "School Name",
          "description": "Name of University or College",
          "identity": false,
          "readonly": false,
          "type": "string"
        },
        "data.city": {
          "title": "City",
          "description": "City where school located",
          "identity": false,
          "readonly": false,
          "type": "string"
        },
        "data.state": {
          "title": "State ",
          "description": "State where school located",
          "identity": false,
          "readonly": false,
          "type": "string"
        },
        "data.zip": {
          "title": "Zip",
          "description": "Zip where school located",
          "identity": false,
          "readonly": false,
          "type": "string"
        },
        "data.totalEnrollment": {
          "title": "Total Enrollment",
          "description": "Total Enrollment",
          "identity": false,
          "readonly": false,
          "type": "number"
        },
        "data.applicationFee": {
          "title": "Application Fee",
          "description": "The cost of applying to school",
          "identity": false,
          "readonly": false,
          "type": "number"
        },
        "data.totalUndergraduateEnrollment": {
          "title": "Total Undergraduate Enrollment",
          "description": "The number of undergraduates enrolled",
          "identity": false,
          "readonly": false,
          "type": "number"
        },
        "data.percentInternationalStudents": {
          "title": "Percentage International Students",
