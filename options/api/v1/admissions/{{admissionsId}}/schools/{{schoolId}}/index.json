      "links": [
        {
          "title": "Un-claim this School",
          "description": "{{data.institutionName}}",
          "rel": "claim",
          "importance": "callToAction",
          "method": "DELETE",
          "href": "/api/v1/admissions/{{admissionsId}}/search/schools/{{_id}}/claim"
        }
      ]
    },
    "applicants": {
      "type": "object",
      "properties": {
        "result": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "_id": {
                "type": "string"
              },
              "fullName": {
                "type": "string"
              },
              "email": {
                "type": "string"
              }
            }
          }
        },
        "meta": {
          "links": [
            {
              "title": "{{page}}",
              "rel": "{{rel}}",
              "method": "GET",
              "importance": "paging",
              "href": "/api/v1/admissions/{{admissionsId}}/schools/{{schoolId}}?{{query}}"
            }
          ]
        }
      },
      "links": [
        {
          "title": "{{page}}",
          "rel": "{{rel}}",
          "method": "GET",
          "importance": "applicants",
          "template": "/templates/api/v1/card/admissions/applicant",
          "href": "/api/v1/admissions/{{admissionsId}}/schools/{{schoolId}}/applicants/{{_id}}"
        }
      ]
    }
